<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>CheckLogin</servlet-name>
        <servlet-class>eas.asu.edu.snac.activitymanager.servlets.CheckLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessNewWish</servlet-name>
        <servlet-class>eas.asu.edu.snac.activitymanager.servlets.ProcessNewWish</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessNewInvitation</servlet-name>
        <servlet-class>eas.asu.edu.snac.activitymanager.servlets.ProcessNewInvitation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterNewUser</servlet-name>
        <servlet-class>eas.asu.edu.snac.activitymanager.servlets.RegisterNewUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JoinInvitation</servlet-name>
        <servlet-class>eas.asu.edu.snac.activitymanager.servlets.JoinInvitation</servlet-class>
       </servlet>
       <servlet>
        <servlet-name>ProcessInvitationRequest</servlet-name>
        <servlet-class>eas.asu.edu.snac.activitymanager.servlets.ProcessInvitationRequest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LeaveInvitation</servlet-name>
        <servlet-class>eas.asu.edu.snac.activitymanager.servlets.LeaveInvitation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>eas.asu.edu.snac.activitymanager.servlets.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckLogin</servlet-name>
        <url-pattern>/CheckLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessNewWish</servlet-name>
        <url-pattern>/ProcessNewWish</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessNewInvitation</servlet-name>
        <url-pattern>/ProcessNewInvitation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterNewUser</servlet-name>
        <url-pattern>/RegisterNewUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessInvitationRequest</servlet-name>
        <url-pattern>/ProcessInvitationRequest</url-pattern>
        </servlet-mapping>
        <servlet-mapping>
        <servlet-name>JoinInvitation</servlet-name>
        <url-pattern>/JoinInvitation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LeaveInvitation</servlet-name>
        <url-pattern>/LeaveInvitation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
